&flash0 {
	partitions {
		app_state_partition: partition@10000 {
			label = "application-state";
			reg = <0x00010000 0x0000b000>;
		};

		credentials_partition: partition@1b000 {
			label = "lwm2m-credentials";
			reg = <0x00018000 0x00001000>;
		};

		syslog_partition: partition@1c000 {
			label = "syslog";
			reg = <0x00019000 0x00004000>;
		};
	};
};

&i2c0 {
	fxos8700@1d {
		label = "fota-temp";
	};
};
